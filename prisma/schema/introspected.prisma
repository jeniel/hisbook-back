model chats_becker {
  id         BigInt @id @default(autoincrement())
  session_id String @db.VarChar(255)
  message    Json?

  @@schema("public")
}

model documents_becker {
  id        BigInt                 @id @default(autoincrement())
  content   String?
  metadata  Json?
  embedding Unsupported("vector")?

  @@schema("public")
}

model chats_callinbound {
  id         BigInt @id @default(autoincrement())
  session_id String @db.VarChar(255)
  message    Json?

  @@schema("public")
}

model documents_callinbound {
  id        BigInt                 @id @default(autoincrement())
  content   String?
  metadata  Json?
  embedding Unsupported("vector")?

  @@schema("public")
}

model chats_demo {
  id         BigInt    @id @default(autoincrement())
  session_id String    @db.VarChar(255)
  message    Json?
  created_at DateTime? @default(now()) @db.Timestamp(6)

  @@schema("public")
}

model documents_demo {
  id         BigInt                 @id @default(autoincrement())
  content    String?
  metadata   Json?
  embedding  Unsupported("vector")?
  created_at DateTime?              @default(now()) @db.Timestamp(6)

  @@schema("public")
}

model keywords {
  id       String  @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  keyword  String  @unique
  count    Int?    @default(1)
  tenantId String? @map("tenant_id")
  tenant   Tenant? @relation(fields: [tenantId], references: [id])

  @@index([tenantId])
  @@schema("public")
}

model keyword_daily {
  id       String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  keyword  String
  count    Int?      @default(1)
  date     DateTime? @default(dbgenerated("CURRENT_DATE")) @db.Date
  tenantId String?   @map("tenant_id")
  tenant   Tenant?   @relation(fields: [tenantId], references: [id])

  @@unique([keyword, date])
  @@index([tenantId])
  @@schema("public")
}
